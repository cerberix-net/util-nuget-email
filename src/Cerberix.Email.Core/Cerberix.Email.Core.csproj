<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RuntimeIdentifiers>win7-x64</RuntimeIdentifiers>
    <Authors>pacjman@cerberix.net</Authors>
    <Company>cerberix.net</Company>
    <Description>core email module for dot net standard</Description>
    <RepositoryUrl>https://github.com/cerberix-net/util-nuget-email/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Version>1.0.0-pre06</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/cerberix-net/util-nuget-email/blob/master/LICENSE</PackageLicenseUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/cerberix-net/util-nuget-email/master/img/cerberix.png</PackageIconUrl>
    <PackageId>Cerberix.Email.Core</PackageId>
    <Product>Cerberix.Email.Core</Product>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
  </PropertyGroup>

</Project>
